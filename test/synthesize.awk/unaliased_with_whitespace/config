# Whether or not to format @synthesize statements
# Possible values:
#   1 - format @synthesize statements
#   2 - ignore @synthesize statements so they pass through
SYNTHESIZERS_SHOULD_BE_FORMATTED = 1 

# The number of columns to format @synthesize statements into
# Possible values:
#   1 - force all synthesizers as far left as possible
#       @synthesize property = _property;
#       @synthesize anotherProperty = _anotherProperty;
#
#   2 - line up lhs and rhs of the assignment
#       @synthesize property        = _property;
#       @synthesize anotherProperty = _anotherProperty;
SYNTHESIZERS_SHOULD_HAVE_N_COLUMNS = 1
